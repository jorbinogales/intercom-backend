{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/$1",
    "@app/gateway-library/(.*)": "<rootDir>/../libs/gateway-library/src/$1",
    "@app/gateway-library": "<rootDir>/../libs/gateway-library/src",
    "@app/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@app/database": "<rootDir>/../libs/database/src"
  }
}